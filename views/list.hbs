<!-- Main content -->
<section class="content">
<div class="row">
    <div class="col-md-6 col-xs-12 connectedSortable">
        <div class="box">
            <div class="box-header">
                <h3 class="box-title">Top Active Tenants</h3>
            </div><!-- /.box-header -->
            <div class="box-body table-responsive">
                <table id="example1" class="table table-bordered table-hover">
                    <thead>
                    <tr>
                        <th style="width: 10px">#</th>
                        <th>Tenant ID</th>
                        <th>Company ID</th>
                        <th>Company Name</th>
                        <th style="width: 40px">Number</th>
                    </tr>
                    </thead>
                    <tbody>
                    {{#each todayActiveTenants}}
                        <tr>
                            <td>{{addOne @index}}.</td>
                            <td>{{this.tenantId}}</td>
                            <td>{{this.companyId}}</td>
                            <td>{{this.companyName}}</td>
                            <td><span class="badge bg-red">{{this.count}}</span></td>
                        </tr>
                    {{/each}}
                    </tbody>
                    <tfoot>
                    <tr>
                        <th style="width: 10px">#</th>
                        <th>Tenant ID</th>
                        <th>Company ID</th>
                        <th>Company Name</th>
                        <th style="width: 40px">Number</th>
                    </tr>
                    </tfoot>
                </table>
            </div><!-- /.box-body -->
        </div><!-- /.box -->
    </div>

    <div class="col-md-6 col-xs-12 connectedSortable">
        <div class="box">
            <div class="box-header">
                <h3 class="box-title">Top Active Users</h3>
            </div><!-- /.box-header -->
            <div class="box-body table-responsive">
                <table id="example2" class="table table-bordered table-hover">
                    <thead>
                    <tr>
                        <th style="width: 10px">#</th>
                        <th>Company ID</th>
                        <th>Company Name</th>
                        <th style="width: 40px">Amount</th>
                    </tr>
                    </thead>
                    <tbody>
                    {{#each groupUsers}}
                        <tr>
                            <td>{{addOne @index}}.</td>
                            <td>{{this.id}}</td>
                            <td>{{this.name}}</td>
                            <td><span class="badge bg-light-blue">{{this.count}}</span></td>
                        </tr>
                    {{/each}}
                    </tbody>
                    <tfoot>
                    <tr>
                        <th style="width: 10px">#</th>
                        <th>Company ID</th>
                        <th>Company Name</th>
                        <th style="width: 40px">Amount</th>
                    </tr>
                    </tfoot>
                </table>
            </div><!-- /.box-body -->
        </div><!-- /.box -->
    </div>
</div>

<div class="row">
    <div class="col-xs-12 connectedSortable">
        <div class="box">
            <div class="box-header">
                <h3 class="box-title">Active User List</h3>
            </div><!-- /.box-header -->
            <div class="box-body table-responsive">
                <table id="example3" class="table table-bordered table-hover">
                    <thead>
                    <tr>
                        <th>#</th>
                        <th>ID</th>
                        <th>Company</th>
                        <th>User</th>
                        <th>Email</th>
                        <th>Phone</th>
                        <th>LastLogin</th>
                        <th>UserCreate</th>
                        <th>KeyUser</th>
                    </tr>
                    </thead>
                    <tbody>
                    {{#each todayActiveUsers}}
                        <tr>
                            <td>{{addOne @index}}.</td>
                            <td>{{this.customerId}}</td>
                            <td>{{this.customerName}}</td>
                            <td>{{this.name}}</td>
                            <td>{{this.email}}</td>
                            <td>{{this.phone}}</td>
                            <td>{{this.lastLoginDate}}</td>
                            <td>{{this.userCreateDate}}</td>
                            {{#if this.isKeyUser}}
                                <td><span class='label label-success'>Yes</span></td>
                            {{else}}
                                <td><span class='label label-danger'>No</span></td>
                            {{/if}}
                        </tr>
                    {{/each}}
                    </tbody>
                    <tfoot>
                    <tr>
                        <th>#</th>
                        <th>ID</th>
                        <th>Company</th>
                        <th>User</th>
                        <th>Email</th>
                        <th>Phone</th>
                        <th>LastLogin</th>
                        <th>UserCreate</th>
                        <th>KeyUser</th>
                    </tr>
                    </tfoot>
                </table>
            </div><!-- /.box-body -->
        </div><!-- /.box -->
    </div>
</div>
</section><!-- /.content -->